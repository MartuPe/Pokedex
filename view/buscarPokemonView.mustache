<div>
    <table style='border-collapse: collapse; width: 100%'>
        <tr style='background-color: #f2f2f2'>
            <th style='border: 1px solid #ddd; padding: 8px'>Imagen Pokemon</th>
            <th style='border: 1px solid #ddd; padding: 8px'>Imagen Tipo</th>
            <th style='border: 1px solid #ddd; padding: 8px'>Numero</th>
            <th style='border: 1px solid #ddd; padding: 8px'>Nombre</th>
            <th style='border: 1px solid #ddd; padding: 8px'>Tipo</th>
        </tr>
        {{#pokemon}}
            <tr style='background-color: #fff'>
                <td style='border: 1px solid #ddd; padding: 8px'><img src="{{imagenPokemon}}" width="100" height="100"></td>
                <td style='border: 1px solid #ddd; padding: 8px'><img src="{{imagenTipo}}" width="100" height="100"></td>
                <td style='border: 1px solid #ddd; padding: 8px'>{{numero}}</td>
                <td style='border: 1px solid #ddd; padding: 8px'>{{nombre}}</td>
                <td style='border: 1px solid #ddd; padding: 8px'>{{tipo}}</td>
            </tr>
        {{/pokemon}}
    </table>
</div>